[global_config]
[keybindings]
  next_tab = <Primary>KP_9
  prev_tab = <Primary>KP_3
[profiles]
  [[default]]
    allow_bold = False
    background_color = "#141d2b"
    background_darkness = 1.0
    background_type = transparent
    font = Monospace 15
    scrollback_infinite = True
    palette = "#14141D1C2B2B:#FFFF3E3E3E3E:#6F6FA7A70000:#B5B57C7B0000:#00008685FFFF:#9F9E0000FFFF:#0000D4D49C9C:#A4A4B1B1CDCD:#181720202B2B:#FFFF5D5D5D5D:#9F9FEFEF0000:#FFFFAFAF0000:#2C2C9B9BFFFF:#ACAB2423FEFE:#2E2EE7E7B6B6:#B5B5BFBFD4D4"
    login_shell = True
    use_custom_command = True
    custom_command = bash
    use_system_font = False
    title_transmit_bg_color = "#bf3f3f"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
